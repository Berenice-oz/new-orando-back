{% extends 'back/base_back.html.twig' %}

{% block title %}Modifier une randonnée{% endblock %}

{% block body %}

{{ include('back/_nav_back.html.twig') }}

<div class="container">

<h3>Modifier la randonnée: {{ walk.title }}</h3>
  


{{ form_start(form, {attr: {novalidate: 'novalidate'}})}}


    
    <div class="form_group">
        <div>
            {{ form_row(form.status) }}
        </div>
    </div>

    <button class="btn btn-lg btn-success" type="submit">
        Enregistrer
    </button>
    
    
   {{ form_end(form) }}

</div>

{% endblock %}